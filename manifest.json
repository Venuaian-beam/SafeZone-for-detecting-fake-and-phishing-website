{
  "manifest_version": 3,
  "name": "SafeZone",
  "version": "1.0",
  "description": "Scan URLs for phishing threats",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": ["http://localhost:5000/*"],
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "final.html"
  }
}
